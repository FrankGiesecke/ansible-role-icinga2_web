;{{ ansible_managed }}
; Configuration from Icingaweb2 version 2.6.2-1

[icingaweb_db]
type = "db"
db = "mysql"
host = "{{ icinga2_web_icingaweb2_database_host }}"
port = "{{ icinga2_web_icingaweb2_database_port }}"
dbname = "{{ icinga2_web_icingaweb2_database_name }}"
